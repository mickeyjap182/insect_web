        <div class="panel panel-info">
          <div id="panel_heading" class="panel-heading panel-info">ログイン</div>
          <form id="user_info" name="user_info" method="post" action="/authenticate">
            <%= hidden_field_tag "authenticity_token"  %>
            <div id="panel_body" class="panel-body">
                <div class="panel-body">
                  <% flash.each do |key, value| %>
                    <div class="alert alert-<%= key %>">
                      <%= value %>
                    </div>
                  <% end %>
                  <div class="container-fluid">
                    <div class="form-group row">
                      <label for="nickname">ニックネーム</label><input class="form-control" type="text" id="nickname" placeholder="例) Bob" name="nickname" value="<%= @nickname %>"  />
                    </div>
                    <div class="form-group row">
                      <label for="password">パスワード</label><input class="form-control"  type="password" id="password" placeholder="" name="password" />
                    </div>
                  </div>
                </div>
            </div>
            <div class="panel-footer">
              <button type="submit" id="login" type="button" class="btn btn-info" >ログイン</button>
            </div>
          </form>
        </div>
      <div id="app">
      </div>
    </div>
